  <body>
    <div class="container">
      <div class="row" >
        <div class="col-xs-12 col-sm-6">
          <div class="panel panel-default">
            <div class="panel-body">
              <button id="findAll" type="button" class="btn btn-info btn-block" onclick="javascript:findAll()">用户列表</button>
            </div>
          </div>
        </div>
        <div class="col-xs-12 col-sm-6">
          <button id="getByCode" type="button" class="btn btn-info btn-block" onclick="javascript:getByCode()">查找用户</button>
        </div>
        
        <div class="clearfix visible-xs"></div>
        
        <div class="col-xs-12 col-sm-6">
           <button type="button" class="btn btn-info btn-block">增加用户</button>
        </div>
        <div class="col-xs-12 col-sm-6">
           <button type="button" class="btn btn-info btn-block">删除用户</button>
        </div>
      </div>
    </div>
